{
  "name": "astro-ultimate-tutorial",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "PUBLIC_ENV=preview astro dev",
    "build:production": "npm run generate:tag-analytics:check && PUBLIC_ENV=production astro build",
    "build:preview": "npm run generate:tag-analytics:check && PUBLIC_ENV=preview astro build",
    "start": "astro dev",
    "build": "npm run generate:tag-analytics && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "generate:tag-analytics": "tsx src/scripts/generate-tag-analytics.ts",
    "generate:tag-analytics:check": "test -f public/tag-analytics.json && find public/tag-analytics.json -mmin -60 | grep -q . || npm run generate:tag-analytics",
    "storyblok:generate-types": "storyblok pull-components --space 332224 && storyblok generate-typescript-typedefs --sourceFilePaths ./components.332224.json --destinationFilePath ./src/types/component-types-sb.d.ts"
  },
  "devDependencies": {
    "@storyblok/astro": "^7.2.4",
    "@tailwindcss/typography": "^0.5.16",
    "astro": "^5.13.2",
    "dotenv": "^17.2.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.4",
    "vite": "^7.1.3",
    "vite-plugin-mkcert": "^1.17.8"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@astrojs/node": "^9.4.2",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.5.0",
    "@lucide/astro": "^0.540.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "astro-remote": "^0.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.540.0",
    "lucide-static": "^0.540.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storyblok-js-client": "^7.1.3",
    "storyblok-rich-text-astro-renderer": "^2.8.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.7"
  }
}
